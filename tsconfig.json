{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "rootDir": "./",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "outDir": ".build"
  },
  "exclude": ["node_modules"]
}
